<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="Main" style="flex-direction: row; flex-grow: 1; border-top-width: 3px; border-right-width: 0; border-bottom-width: 0; border-left-width: 0; background-color: rgba(103, 109, 113, 0); -unity-background-image-tint-color: rgb(255, 255, 255); -unity-background-scale-mode: stretch-to-fill; display: flex; opacity: 1; flex-wrap: nowrap; text-overflow: clip; -unity-text-align: upper-left; align-self: auto; border-left-color: rgb(47, 47, 47); border-right-color: rgb(47, 47, 47); border-top-color: rgb(47, 47, 47); border-bottom-color: rgb(47, 47, 47); border-top-left-radius: 2px; border-top-right-radius: 2px; border-bottom-right-radius: 2px; border-bottom-left-radius: 2px;">
        <ui:VisualElement name="Tabs" style="width: 208px; margin-right: 10px; height: 3039px; background-color: rgba(0, 0, 0, 0); border-left-color: rgb(47, 47, 47); border-right-color: rgb(47, 47, 47); border-top-color: rgb(47, 47, 47); border-bottom-color: rgb(47, 47, 47); border-top-width: 0; border-right-width: 2px; border-bottom-width: 0.8px; border-left-width: 0; margin-top: 0; justify-content: flex-start; -unity-slice-left: 0; border-top-left-radius: 0.5px; border-top-right-radius: 0.5px; border-bottom-right-radius: 0.5px; border-bottom-left-radius: 0.5px; align-items: stretch; flex-direction: column; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0;">
            <ui:Button text="Information" display-tooltip-when-elided="true" name="StartTabButton" style="background-color: rgb(128, 128, 128);" />
            <ui:Button text="Update SDK" display-tooltip-when-elided="true" name="UpdateSDKTabButton" style="background-color: rgb(128, 128, 128);" />
            <ui:Button text="Mod Management" display-tooltip-when-elided="true" name="CreateModTabButton" style="display: flex; background-color: rgb(128, 128, 128);" />
            <ui:Button text="Steam Workshop" display-tooltip-when-elided="true" name="SteamWorkshopTabButton" style="display: flex; background-color: rgb(128, 128, 128);" />
            <ui:Button text="Mod.IO" display-tooltip-when-elided="true" name="LogInTabButton" style="margin-top: 20px; margin-right: 15px; margin-left: 15px; min-width: 100px; background-color: rgb(128, 128, 128);" />
            <ui:Button text="Search Mods" display-tooltip-when-elided="true" name="SearchModTabButton" style="display: none; background-color: rgb(128, 128, 128);" />
            <ui:Button text="Mod.IO" display-tooltip-when-elided="true" name="UploadModTabButton" style="display: none; background-color: rgb(128, 128, 128);" />
        </ui:VisualElement>
        <ui:VisualElement name="Content" style="min-width: 350px;">
            <ui:Label text="Welcome to Core Keepers&apos; ModSDK!" name="Title" style="align-items: center; justify-content: center; -unity-text-align: upper-center; -unity-font-style: bold; font-size: 18px; color: rgb(210, 210, 210); margin-bottom: 10px;" />
            <ui:VisualElement name="StartView" style="visibility: visible; flex-grow: 1; overflow: visible; display: flex;">
                <ui:Label text="First go to the &lt;link=UpdateSDKTabButton&gt;Update SDK&lt;/link&gt; menu and pull in the necessary game files from the &lt;i&gt;Core Keeper&lt;/i&gt; game folder, afterwards follow the Asset Ripper workflow from the GitBook modding guide.&#10;&#10;Once you&apos;ve updated the SDK and the Assets you can feel free to check out the examples mods made by us by extracting the Assets/Examples zip file into the same folder as the zip.&#10;&#10;You can create a Mod in the &lt;link=ModTabButton&gt;Mod Management&lt;/link&gt; menu. The typical workflow is to name and create the mod, which will construct the basic mod structure in the project at &quot;Assets/Mod_Name&quot;. Then you can add and modify mod files as you wish to create the desired functionality.&#10;&#10;Once you&apos;re ready to upload your mod you can head over to either the Steam Workshop or the Mod.IO page, depending on which platform you want to upload the mod to. Do the upload step again when you want to update the mod." display-tooltip-when-elided="true" name="StartViewText" enable-rich-text="true" style="-unity-font-style: normal; white-space: normal; -unity-text-align: upper-center; margin-right: 10px; margin-left: 10px; width: 320px; height: 400px; min-width: 320px; max-width: 320px; min-height: 400px; max-height: 400px;" />
            </ui:VisualElement>
            <ui:VisualElement name="UpdateSDKView" style="visibility: visible; flex-grow: 1; display: none;">
