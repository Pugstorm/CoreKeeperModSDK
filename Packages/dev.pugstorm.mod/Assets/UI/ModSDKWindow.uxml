                    <ui:Image name="SteamThumbnailUpload" style="width: 280px; height: 160px; margin-left: 30px; margin-top: 12px; margin-bottom: 12px; margin-right: 0; -unity-background-scale-mode: scale-and-crop;" />
                    <ui:Button text="Upload to Steam Workshop" display-tooltip-when-elided="true" name="SteamUploadModButton" />
                </ui:VisualElement>
