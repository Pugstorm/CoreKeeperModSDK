                                <ui:TextField picking-mode="Ignore" name="UploadModSummary" multiline="true" value="Describe mod here with a short summary." style="margin-top: 5px; margin-bottom: 5px; width: 200px; height: auto; white-space: normal; min-height: 50px;" />
                            </ui:VisualElement>
                        </ui:VisualElement>
                        <ui:VisualElement name="UploadModLogo" style="width: 80px; height: 45px; margin-left: 45px; margin-top: 7px;">
                            <ui:Button display-tooltip-when-elided="true" tooltip="Press to change logo" name="UploadModLogoButton" style="display: flex; visibility: visible; height: 45px; width: 80px; opacity: 0;" />
                        </ui:VisualElement>
                    </ui:VisualElement>
                    <ui:TextField picking-mode="Ignore" label="URL" name="UploadModURL" readonly="false" style="display: none;" />
                    <ui:TextField picking-mode="Ignore" label="Tags" name="UploadModTags" readonly="false" style="display: none;" />
                    <ui:Button text="Register at mod.io" display-tooltip-when-elided="true" name="CreateModProfile" />
                    <ui:Button text="Go to mod page" display-tooltip-when-elided="true" name="GoToProfileButton" />
                    <ui:Button text="Upload to mod.io" display-tooltip-when-elided="true" name="UploadModButton" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="CreateModView" style="visibility: visible; display: none;">
                <ui:DropdownField label="Mod Select -" index="-1" name="CreateModModList" choices="New Mod" style="margin-bottom: 2px; left: 0; width: 387px; height: 22px; margin-top: 4px; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; -unity-text-align: middle-right; margin-left: -48px; font-size: 12px; margin-right: 14px;" />
                <ui:VisualElement name="CreateModForm">
                    <ui:TextField picking-mode="Ignore" label="Mod Name -" name="CreateModName" readonly="false" style="min-width: 300px; left: 0; -unity-text-align: middle-right; margin-top: 5px; margin-left: -48px; margin-right: 12px; font-size: 12px;" />
                    <ui:VisualElement style="align-items: center; margin-top: 10px;">
                        <ui:Button text="Create Mod" display-tooltip-when-elided="true" name="CreateModButton" style="width: 266px; left: 72px; height: 22px; margin-left: 0; margin-right: -45px; -unity-text-align: middle-center; align-self: flex-start; margin-top: 4px;" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="LocalExportForm">
                    <ui:VisualElement name="ModSettings">
                        <ui:Toggle label="Require for multiplayer" name="RequiredOnServerAndClient" tooltip="Will enforce the mod to be present for connecting clients as well" />
                        <ui:ListView focusable="true" name="ModDependencies" show-foldout-header="true" show-add-remove-footer="true" header-title="Dependencies" reorderable="true" reorder-mode="Simple" show-border="true" virtualization-method="DynamicHeight" show-alternating-row-backgrounds="All" />
                    </ui:VisualElement>
                    <ui:Label display-tooltip-when-elided="true" name="LocalExportGamePath" text="Go to &apos;Update SDK&apos; to set correct path for mod loading." style="margin-left: 0%; white-space: normal;" />
                    <ui:Button text="Build Mod" display-tooltip-when-elided="true" name="BuildingButton" />
                    <ui:Button text="Install Mod" display-tooltip-when-elided="true" name="InstallModButton" />
                    <ui:Button text="Build and Install Mod" display-tooltip-when-elided="true" name="LocalExportButton" />
                    <ui:Button text="Open Last Build/Install Folder" display-tooltip-when-elided="true" name="OpenLastBuildFolderButton" />
