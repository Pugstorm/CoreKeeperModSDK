                    <ui:Label display-tooltip-when-elided="true" name="SteamExportGamePath" style="margin-left: 5px;" />
                    <ui:TextField picking-mode="Ignore" label="Title" name="SteamWorkshopFolderName" />
                    <ui:TextField picking-mode="Ignore" label="Description" name="SteamUploadModSummary" />
                    <ui:TextField picking-mode="Ignore" label="File ID" name="SteamWorkshopFileID" tooltip="Set to 0 for a new file, set to Steam Workshops&apos; exact file ID if updating" />
                    <ui:DropdownField label="Mod Visibility" name="SteamVisibility" text="Select Tag for Steam Workshop" style="width: initial; align-self: auto; align-items: auto; margin-left: 2px; justify-content: flex-start; flex-direction: row; margin-right: 3px; margin-bottom: 0; margin-top: 0;" />
                    <ui:EnumField label="Tag Type" value="Center" type="UnityEngine.TextAlignment, UnityEngine.TextRenderingModule" name="SteamWorkshopTagType" />
                    <ui:DropdownField label="Tags" name="SteamWorkshopTags" text="Select Tag for Steam Workshop" style="width: initial; align-self: auto; align-items: auto; margin-left: 2px; justify-content: flex-start; flex-direction: row; margin-right: 3px; margin-bottom: 0; margin-top: 0;" />
                    <ui:VisualElement name="SteamWorkshopTagsList" picking-mode="Ignore" style="flex-direction: row; flex-wrap: wrap; align-items: flex-start; margin-top: 6px; margin-bottom: 20px; width: auto; height: 26px; min-height: auto; position: relative; display: flex; visibility: visible; opacity: 1; max-height: none; min-width: auto; max-width: none; background-size: 100% 100%; padding-left: 4px; padding-right: 4px; font-size: 10%; -unity-text-align: upper-center; padding-bottom: 0; -unity-font-style: bold;">
                        <ui:Button text="Button" name="TagBase" picking-mode="Ignore" style="margin-right: 2px; margin-bottom: 2px; display: none; opacity: 0.8; width: 75%; height: 75%; min-width: 75%; min-height: 75%; max-height: 75%; max-width: 75%; background-color: rgb(173, 232, 243); background-size: 75% 75%;" />
                    </ui:VisualElement>
