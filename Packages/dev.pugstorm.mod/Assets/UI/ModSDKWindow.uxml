                <ui:DropdownField label="Install Path -" index="-1" name="UpdateSDKChooseGamePath" style="-unity-text-align: middle-right; left: -50px; margin-top: 40px; width: 380px; height: 22px; min-width: 380px; min-height: 22px; max-width: 380px; max-height: 22px; -unity-font-style: normal; font-size: 12px;" />
                <ui:VisualElement name="UpdateSDKButtons" style="align-items: stretch; margin-top: 5px; margin-left: 0; max-width: none; max-height: none; width: auto; height: auto; min-height: auto; min-width: auto; -unity-text-align: middle-center; left: 0;">
                    <ui:Button text="Select path to Core Keeper Install" display-tooltip-when-elided="true" name="UpdateSDKChooseGamePathManually" style="margin-top: 10px; width: 257px; min-width: 257px; white-space: normal; max-width: 257px; max-height: 22px; min-height: 22px; height: 22px; position: relative; top: auto; left: 72px; right: auto; bottom: auto; font-size: 12px;" />
                    <ui:Button text="Update Game Files" display-tooltip-when-elided="true" name="UpdateSDKUpdateButton" style="margin-top: 4px; width: 257%; min-width: 257px; white-space: normal; left: 72px; height: 22px; max-width: 257px; min-height: 22px; max-height: 22px; font-size: 12px;" />
                </ui:VisualElement>
                <ui:DropdownField label="Assets Path -" index="-1" name="UpdateAssetsChooseAssetRipperPath" style="-unity-text-align: middle-right; left: -50px; margin-bottom: 2px; margin-top: 50px; width: 380px; height: 22px; min-width: 380px; max-width: 380px; min-height: 22px; max-height: 22px; -unity-font-style: normal; font-size: 12px;" />
                <ui:VisualElement name="UpdateAssetsButtons" style="align-items: stretch; margin-top: 5px; -unity-text-align: middle-center; left: 0;">
                    <ui:Button text="Select path to Core Keeper Assets" display-tooltip-when-elided="true" name="UpdateAssetsChooseGamePathManually" style="margin-top: 10px; width: 257px; min-width: 257px; white-space: normal; max-width: 257px; height: 22px; min-height: 22px; max-height: 22px; left: 72px; -unity-font-style: normal; font-size: 12px; background-image: none; background-color: rgb(88, 88, 88);" />
                    <ui:Button text="Update Game Assets" display-tooltip-when-elided="true" name="UpdateAssetsUpdateButton" style="margin-top: 4px; width: 257px; min-width: 257px; white-space: normal; left: 72px; max-width: 257px; height: 22px; min-height: 22px; max-height: 22px; -unity-font-style: normal; font-size: 12px; background-color: rgb(88, 88, 88); background-image: none;" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="SearchModView" style="visibility: visible; flex-grow: 1; display: none;">
